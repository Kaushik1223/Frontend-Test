const handleShowResult = () => {
    const jsonMessages = events
      .filter(event => typeof event.message === 'object') // Filter events with JSON messages
      .map(event => JSON.stringify(event.message, null, 2)) // Convert each JSON message to string
      .join('\n'); // Join the JSON strings with newlines

    setMessage(jsonMessages); // Set the JSON strings to the state
    setShowModal(true); // Show the modal
  };
